<h1>Salesforce Accounts</h1>

<table class="table">
  <tr>
    <th>Id</th>
    <th>Name</th>
    <th>Productcode</th>
    <th>Description</th>
    <th>Family</th>
  </tr>
  {{#each records}}
    <tr>
      <td>{{get 'id'}}</td>
      <td>{{get 'Name'}}</td>
      <td>{{get 'Productcode'}}</td>
      <td>{{get 'Description'}}</td>
      <td>{{get 'Family'}}</td>
    </tr>
  {{/each}}
</table>
